<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:ffimg="clr-namespace:FFImageLoading.Svg.Forms;assembly=FFImageLoading.Svg.Forms"
             x:Class="IncliForms.Controls.AddDeviceButton"
             HeightRequest="232"
             WidthRequest="190"
             Padding="0"
             Margin="0">
    <ffimg:SvgCachedImage Source="resource://IncliForms.Resources.add_device.svg"
                          HeightRequest="232"
                          WidthRequest="190"
                          Margin="0">
        <ffimg:SvgCachedImage.GestureRecognizers>
            <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped"/>
        </ffimg:SvgCachedImage.GestureRecognizers>
    </ffimg:SvgCachedImage>
</ContentView>